{
  "name": "password-generator",
  "version": "0.0.0",
  "description": "Yet just another password generator",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "mocha": "mocha --reporter spec",
    "cover": "istanbul cover _mocha -- -R spec test/*",
    "test": "gulp test && npm run mocha && npm run cover",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "preversion": "npm test",
    "postversion": "git push && git push --tags && npm publish"
  },
  "pre-commit": [
    "precommit-msg",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luanmuniz/password-generator.git"
  },
  "keywords": [
    "password",
    "random",
    "string"
  ],
  "author": "Luan Muniz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luanmuniz/password-generator/issues"
  },
  "homepage": "https://github.com/luanmuniz/password-generator#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "gulp": "^3.9.1",
    "gulp-david": "^1.0.0",
    "gulp-eslint": "^3.0.0",
    "gulp-jshint": "^2.0.1",
    "istanbul": "^0.4.4",
    "jshint": "^2.9.2",
    "mocha": "^2.5.3",
    "pre-commit": "^1.1.3"
  },
  "dependencies": {
    "zxcvbn": "^4.3.0"
  }
}
